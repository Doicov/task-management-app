/**!!!
* If you intent to modify all front end apps edit this file in template-vitemf-react repo
* !!!
*/
{
    "compilerOptions": {
      "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
      "target": "ES2020",
      "useDefineForClassFields": true,
      "lib": ["ES2020", "DOM", "DOM.Iterable"],
      "module": "ESNext",
      "skipLibCheck": true,
   
      /* Bundler mode */
      "moduleResolution": "bundler",
      "isolatedModules": true,
      "moduleDetection": "force",
      "jsx": "react-jsx",
      "composite": true,
   
      /* Linting */
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noFallthroughCasesInSwitch": true,
    },
    "extends": "./tsconfig.json",
   
    "include": [
      "src/**/*",
      "generated/**/*",
      "generated/tada/**/*.ts",
   
  ]
  }